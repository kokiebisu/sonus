{
  "manifest_version": 3,
  "name": "Axolotl",
  "version": "1.0",
  "description": "Sends urls via requests to the local running server",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["http://localhost/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
